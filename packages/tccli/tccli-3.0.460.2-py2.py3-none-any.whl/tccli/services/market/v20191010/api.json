          "member": "float",
          "name": "TimeSpan",
          "required": false,
          "type": "float",
          "value_allowed_null": false
        },
        {
          "default": "",
          "document": "商品的时间单位：year:年；month:月；day:日；hour:小时；minute:分钟；second:秒; package:与价格无关,一次性购买的产品传package",
          "member": "string",
          "name": "TimeUnit",
          "required": false,
          "type": "string",
          "value_allowed_null": false
        },
        {
          "default": "",
          "document": "原价 （单位：分）OriginPrice=round(UnitPrice * Dosage * TimeSpan)",
          "member": "int64",
          "name": "OriginPrice",
          "required": false,
          "type": "int",
          "value_allowed_null": false
        },
        {
          "default": "",
          "document": "折扣百分比，传入0-100的值",
          "member": "float",
          "name": "Discount",
          "required": false,
          "type": "float",
          "value_allowed_null": false
        },
        {
          "default": "",
          "document": "最终扣费金额（单位：分）Fee=round(OriginPrice*Discount/100)",
          "member": "int64",
          "name": "Fee",
          "required": false,
          "type": "int",
          "value_allowed_null": false
        }
